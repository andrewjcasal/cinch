    "draft-js": "^0.10.5",
    "jquery": "^3.3.1",
    "moment": "^2.20.1",
    "node-sass-chokidar": "0.0.3",
    "npm-run-all": "^4.1.2",
    "popper.js": "~1.12.9",
    "react": "^16.2.0",
    "react-bootstrap": "^0.32.1",
    "react-datepicker": "^1.2.1",
